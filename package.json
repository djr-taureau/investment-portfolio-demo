{
  "name": "sbproot-ui",
  "version": "0.0.70",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --port=4400",
    "prod": "ng build --prod --build-optimizer",
    "serve-prod": "ng serve --prod --port=5000",
    "build": "ng build",
    "test": "ng test --browsers Chrome",
    "test-once": "ng test --watch=false --browsers Chrome",
    "test-debug": "ng test --browsers ChromeDebugging",
    "ci-test": "ng test --watch=false --browsers ChromeHeadless",
    "test-code-coverage": "ng test --browsers Chrome --code-coverage",
    "serve-test-code-coverage": "http-server -c-1 -o -p 4500 ./coverage",
    "lint": "ng lint",
    "lint-fix": "ng lint sbproot-ui --fix",
    "lint-staged-tslint": "tslint --config ./src/tslint.json --project ./src/tsconfig.pre-commit-hook.json --fix",
    "lint-staged-prettier": "prettier --write \"src/app/**/*.ts\"",
    "lint-staged": "npm run lint-staged-prettier && npm run lint-staged-tslint",
    "format-html": "glob-run js-beautify --type html -r \"src/app/**/*.html\"",
    "format-css-components": "glob-run js-beautify --type css -r \"src/app/**/*.scss\"",
    "format-css": "npm run format-css-components",
    "format": "npm run prettier && npm run format-html && npm run format-css-components && npm run format-css",
    "format-and-lint": "npm run format && npm run lint-fix",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"src/app/**/*.ts\"",
    "prettier-and-lint": "npm run prettier && npm run lint-fix",
    "release": "standard-version",
    "create-version": "node git-version.js",
    "bump": "npm run version-package && npm run create-version && npm run commit-package",
    "version-package": "npm --no-git-tag-version version patch",
    "commit-package": "git add package.json package-lock.json src/version.ts"
  },
  "lint-staged": {
    "*ts": [
      "prettier --write",
      "tslint --config ./src/tslint.json --project ./src/tsconfig.pre-commit-hook.json --fix",
      "git add"
    ],
    "*.html": [
      "js-beautify --type html -r",
      "git add"
    ],
    "*.scss": [
      "js-beautify --type css -r",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.15",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/flex-layout": "7.0.0-beta.24",
    "@angular/forms": "7.2.15",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/router": "7.2.15",
    "@ngrx/effects": "7.4.0",
    "@ngrx/entity": "7.4.0",
    "@ngrx/router-store": "7.4.0",
    "@ngrx/store": "7.4.0",
    "@ngrx/store-devtools": "7.4.0",
    "@progress/kendo-angular-charts": "^3.9.1",
    "core-js": "2.5.4",
    "hammerjs": "^2.0.0",
    "lodash": "^4.17.11",
    "rxjs": "6.5.2",
    "tslib": "1.9.3",
    "zone.js": "0.9.1",
    "@progress/kendo-angular-intl": "^1.4.0",
    "@progress/kendo-angular-l10n": "^1.1.0",
    "@progress/kendo-drawing": "^1.1.2",
    "rxjs-compat": "^6.0.0",
    "@progress/kendo-theme-default": "latest"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@angular/language-service": "7.2.15",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "^4.14.132",
    "@types/node": "12.0.1",
    "codelyzer": "5.0.1",
    "concurrently": "4.1.0",
    "fs-extra": "8.0.1",
    "git-describe": "4.0.4",
    "glob-run": "0.1.6",
    "husky": "2.3.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "js-beautify": "1.10.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-junit-reporter": "1.2.0",
    "lint-staged": "8.1.6",
    "ng-bullet": "1.0.3",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.17.1",
    "protractor": "5.4.2",
    "standard-version": "6.0.1",
    "tslint": "5.16.0",
    "typescript": "3.2.2"
  },
  "engines": {
    "node": "10.15.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run bump && lint-staged",
      "pre-push": "npm run ci-test"
    }
  }
}
